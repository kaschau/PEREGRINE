import inlets
import exits
import walls


def applyBcs(mb, terms):

    # First we apply inlets and exits
    for blk in mb:
        for face in blk.faces:
            bc = face.connectivity["bcType"]
            if bc in ["b0", "b1"]:
                continue
            elif bc == "constant_velocity_subsonic_inlet":
                inlets.constant_velocity_subsonic_inlet(mb.eos, blk, face, mb.thtrdat, terms)
            elif bc == "constant_pressure_subsonic_exit":
                exits.constant_pressure_subsonic_exit(mb.eos, blk, face, mb.thtrdat, terms)
    # Then we apply walls
    for blk in mb:
        for face in blk.faces:
            bc = face.connectivity["bcType"]
            if bc in ["b0", "b1"]:
                continue
            elif bc == "adiabatic_noslip_wall":
                walls.adiabatic_noslip_wall(mb.eos, blk, face, mb.thtrdat, terms)
            elif bc == "adiabatic_slip_wall":
                walls.adiabatic_slip_wall(mb.eos, blk, face, mb.thtrdat, terms)
            elif bc == "adiabatic_moving_wall":
                walls.adiabatic_moving_wall(mb.eos, blk, face, mb.thtrdat, terms)
            elif bc == "isoT_moving_wall":
                walls.isoT_moving_wall(mb.eos, blk, face, mb.thtrdat, terms)
